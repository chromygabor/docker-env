{
  "name": "@chromygabor/docker-env",
  "version": "1.0.0",
  "description": "This is my first module to provide some utils for docker",
  "main": "dist/index.js",
  "bin": "dist/main.js",
  "license": "MIT",
  "author": "Gabor Chromy",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "r": "ts-node src/main.ts",
    "start:watch": "nodemon --exec ts-node src/index.ts",
    "build": "tsc --build tsconfig.json; chmod +x dist/main.js",
    "build:watch": "tsc --build tsconfig.json --watch",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "test": "mocha --timeout 60000",
    "test:watch": "mocha --watch --timeout 60000"
  },
  "private": true,
  "dependencies": {
    "@types/dockerode": "^2.5.34",
    "@types/minimist": "^1.2.0",
    "chai": "^4.2.0",
    "docker-compose": "^0.23.5",
    "dockerode": "^3.2.0",
    "minimist": "^1.2.6",
    "mocha": "^8.0.1"
  },
  "directories": {
    "src": "./src",
    "test": "./test"
  }
}
